ion(accommondations);\n            } else if (menu_choice.equals(\"3\")) {\n                current_user.display();\n            } else {\n                Messages messenger = new Messages(current_user);\n            }\n            else{\n                return;\n            }\n        } else if (current_user.getClass() == Provider.class) {\n            if (menu_choice.equals(\"1\")) {\n                ((Provider)current_user).modifyAccommondations();\n            } else if (menu_choice.equals(\"2\")) {\n                current_user.display();\n            } else {\n                Messages messenger = new Messages(current_user);\n            }\n        } else {\n            if (menu_choice.equals(\"1\")) {\n                ((Administrator)current_user).searchReservations();\n            } else if (menu_choice.equals(\"2\")) {\n                ((Administrator)current_user).searchUsers(users); //ok\n            } else if (menu_choice.equals(\"3\")) {\n                ((Administrator)current_user).approveUsers();\n            } else {\n                Messages messenger = new Messages(current_user);\n            }\n        }\n     }\n\n    public void display(User user)\n     {\n         System.out.println(\"Name: \" + user.get_Name());\n         System.out.println(\"Surname: \" + user.get_Surname());\n         System.out.println(\"Age: \" + user.get_Age());\n         System.out.println(\"Email: \" + user.get_Email());\n         System.out.println(\"Username: \" + user.get_Username());\n\n         System.out.println();\n\n         System.out.println(\"Do you want to make changes?\");\n         System.out.println(\"1: Yes, 2: No\");\n\n         String choice = scanner.nextLine();\n         if(choice.equals(\"1\")){\n             do {\n                 System.out.println(\"1: Change name, 2: Change Surname, 3: Change Age, 4: Change email, 5: Username, 6: Change Password\");\n                 choice = scanner.nextLine();\n\n                 switch (choice){\n            